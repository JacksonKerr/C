#include <stdio.h>
#include <stdlib.h>

int isPrime(int i) {
    int x;
    for (x = 0; x < i/2; x++) {
        if (i % x == 0) {
            return 0;
        }
    }
    return 1;
}


int main() {
    int i = 2;
    int shown = 0;
    
    while (shown < 200) {
        if (isPrime(i)) {
            printf("%6d", i);
        }
        if (shown % 9 == 0) {
            printf("\n");
        }
        shown++;
    }
    
    return EXIT_SUCCESS;
}
