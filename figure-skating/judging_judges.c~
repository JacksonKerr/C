#include <stdio>
#include <stdlib>

#define ARRAY_SIZE 10

int main(void) {
    double judge1[ARRAY_SIZE];
    double judge2[ARRAY_SIZE];
    double judge3[ARRAY_SIZE];
    int lines_read = 0;
    int ent_num;
    int dummy;
    while (4 == scanf("%l%d%d%d", &dummy , &judge1[ent_num], &judge2[ent_num], &judge3[ent_num])) {
        lines_read++;
        
    }
}
